<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Producto</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            
            <% products.forEach(Producto => { %>
            <tr>
              <th scope="row"></th>
              <td><%= Producto.id %></td>
              <td><%= Producto.title %></td>
              <td><%= Producto.price %></td>
              <td>
                <img src=<%= Producto.thumbnail %> alt="img" width="50">
              </td>
            </tr>
            <% }) %>
        
           
        </tr>

    </tbody>

</table>

<div class="text-center">
    <a href="/" class="btn btn-dark">Ingresar Producto</a>
</div>